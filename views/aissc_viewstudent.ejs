  function reply_click(clicked_id,cand_name)
  {
    $('#onclickmodal').modal("toggle");
    $('#onclickmsg').text(`Fetching ${cand_name} details...`);
    setTimeout(function() { 
      $('#onclickmodal').modal("toggle");
     }, 3500);
     globalThis.clicked_id = clicked_id;
    var xhr;
    if(window.XMLHttpRequest){
      xhr = new XMLHttpRequest();
    }else{
      xhr = new ActiveXObject("Microsoft.XMLHTTP");
    }
    xhr.open("POST", "/select_aissc_student", true);
              xhr.setRequestHeader(
                "Content-type",
                "application/x-www-form-urlencoded"
              );
              xhr.onreadystatechange = function () {
                if (xhr.readyState == 4 && xhr.status == 200) {
                  // document.getElementById("response").innerHTML =
                  //   xhr.responseText;
                  
                  const data = JSON.parse(this.responseText);
                   console.log(data.neet[0]);
                    admission(data.admission[0]);
                    pro(data.profile[0]);
                    address(data.address[0]);
                    photos(data.photos);
                    bank_details(data.bank[0]);
                    cont(data.contact[0]);
                    cert_detail(data.certificate)
                    relieving(data.relieving[0]);
                    institute(data.institute[0]);
                    neet(data.neet[0]);
                    fees(data.fees[0]);
                    sure(data.surety[0]),
                    mbbs_marks(data.mbbs_marks[0]),
                    diploma_marks(data.diploma_marks[0]),
                    mdms_marks(data.mdms_marks[0])
                    certificate_files(data.certificate_files);
                    surety_file(data.surety_file[0]);
                 }
                 $('#myModal').modal('show');
              };
              xhr.send("cand_id=" + clicked_id);


  }
