<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Report</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- Font Awesome -->
    <link
      rel="stylesheet"
      href="/assets/dashboard/plugins/fontawesome-free/css/all.min.css"
    />
    <!-- Ionicons -->
    <link
      rel="stylesheet"
      href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css"
    />
    <!-- bootsrtap -->

    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
    <!-- Theme style -->
    <link rel="stylesheet" href="/assets/dashboard/dist/css/alter.css" />
    <link rel="stylesheet" href="/assets/dashboard/dist/css/mbbs.css" />
    <!-- Google Font Source Sans Pro -->
    <link
      href="https://fonts.googleapis.com/css?family=Source+Sans+Pro300,400,400i,700"
      rel="stylesheet"
    />
  </head>
  <style>
    body,
    html,
    li,
    .margin,
    .text-left,
    #center {
      font-family: "Berlin Sans FB", sans-serif !important;
    }
    #tablecerti th {
      font-weight: 600;
    }
    .table th,
    .table td {
      padding: 2px 8px;
    }
    .index th,
    .index td,
    .index thead,
    .index tbody {
      border: none !important;
      font-size: 20px;
      text-align: left;
    }
    .index th {
      font-weight: 600;
    }
    .bordercerti th,
    #borderhead th {
      font-weight: 600;
    }
    .bordercerti td,
    i,
    .bordercerti th,
    #borderhead th {
      font-size: 20px !important;
    }
    @media print {
      body {
        margin-top: 150px !important;
      }
      .bordercerti td,
      .bordercerti th {
        border-bottom: none !important;
      }
      .table-bordered td,
      .table-bordered th {
        border-right: 2px solid !important;
      }
      .table-bordered #tbody td {
        padding: 10px;
      }
    }
  </style>
  <body>
    <section id="login" class="my-5">
      <div class="container">
        <div class="toolbar no-print">
          <button class="btn btn-info" onclick="window.print()">
            Print Certificate
          </button>
          <a href="/report"><button class="btn btn-info">Back</button></a>
        </div>
        <section id="report" class="text-dark" style="color: #000">
          <div class="">
            <div id="reportcard" class="bond">
              <div class="row justify-content-center">
                <div class="col-md-12">
                  <h4 class="other-font text-dark" style="line-height: 40px">
                    <u><%=title%></u>
                  </h4>
                </div>
              </div>

              <div class="row neetcerti mt-3">
                <div class="col-md-12">
                  <div class="table table-responsive">
                    <table class="table table-bordered">
                      <thead id="borderhead" class="text-center">
                        <th>S.no</th>
                        <% for (var i = 0; i < keys.length; i++) { %>
                        <th style="width: 200px"><%=keys[i]%></th>
                        <%}%>
                      </thead>

                      <tbody class="bordercerti">
                        <%for (var j = 0; j < values.length; j++) {%>
                        <tr>
                          <td><%=j + 1%></td>
                          <%for (var k = 0; k < keys.length; k++) {%>
                          <td><%=values[j][keys[k]]%></td>
                          <% }%>
                        </tr>
                        <%}%>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
      </div>
    </section>

    <!-- jQuery bootstrap -->
    <script
      src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
      integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
      integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
      integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
      crossorigin="anonymous"
    ></script>
    <script>
      UPLOADCARE_PUBLIC_KEY = "demopublickey";
    </script>
    <script
      src="https://ucarecdn.com/libs/widget/3.x/uploadcare.full.min.js"
      charset="utf-8"
    ></script>
    <!-- AdminLTE App -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>

    <script src="/assets/js/print.js"></script>
    <script src="js/adminlte.js"></script>
    <script src="js/mbbs.js"></script>

    <!-- AdminLTE for demo purposes -->
    <script src="js/demo.js"></script>
    <!-- page script -->
  </body>
</html>
