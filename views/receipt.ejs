<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Acknowledgement</title>
    <link rel="icon" type="image/png" href="/assets/img/logo.gif" />
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <!-- Font Awesome -->
    <link
      rel="stylesheet"
      href="/assets/dashboard/plugins/fontawesome-free/css/all.min.css"
    />
    <!-- Ionicons -->
    <link
      rel="stylesheet"
      href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css"
    />
    <!-- bootsrtap -->

    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
    <!-- Theme style -->
    <link rel="stylesheet" href="/assets/dashboard/dist/css/alter.css" />
    <link rel="stylesheet" href="/assets/dashboard/dist/css/mbbs.css" />
    <link
      href="http://fonts.cdnfonts.com/css/berlin-sans-fb"
      rel="stylesheet"
    />

    <!-- Google Font Source Sans Pro -->
    <!-- <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro300,400,400i,700" rel="stylesheet"> -->
    <style>
      body,
      html,
      li,
      .margin,
      .text-left,
      #center {
        font-family: "Berlin Sans FB", sans-serif !important;
      }
      .table td,
      .table thead th {
        text-align: left;
       border: 1px solid black!important;
       font-size: 20px;
      }
      li{
        list-style: auto!important;
        font-size: 20px;
      }
      ul{
        left: 0!important;
      }
      @media print {
        body {
          transform: scale(1.2) !important;
        }
      }
    </style>
  </head>

  <body class="hold-transition sidebar-mini">
    <div class="toolbar no-print">
      <button class="btn btn-info" onclick="window.print()">
        Print Certificate
      </button>

      <a href="/logout"><button class="btn btn-info">Back</button></a>
    </div>

    <section id="report" class="blue">
      <div class="container d-flex justify-content-center">
        <div id="reportcard" class="">
          <div class="card mt-5 p-3" id="bordert">
            <div class="row text-center" id="top">
              <div class="col-md-2 text-center">
                <img src="assets/img/r1.jpg" class="img-fluid" width="150" />
              </div>
              <div class="col-md-7" id="topcenter">
                <h2 class="other-font">
                  Tirunelveli Medical College<br /><span class="tvl"
                    >Tirunelveli.</span
                  >
                </h2>
                <h5 class="blue margin" id="state">
                  <span class="blue margin"
                    >STATE OF TAMILNADU, INDIA<BR />PINCODE:627011</span
                  >
                </h5>
                <h5 class="text-center margin" id="addrep1">
                  <strong class="blue margin" id="addrep">
                    Tel: 91-462-2572733, 257273 Fax: 91-462-2572944</strong
                  >
                </h5>
                <p id="newadd" class="margin">
                  Under the Directorate of Medical Education, Government of
                  Tamilnadu, Chennai-10 Affliated with the Tamil Nadu Dr.M.G.R.
                  Medical University, Guindy, Chennai-32 Recognized by the
                  National Medical Council , New Delhi.
                </p>
              </div>
              <div class="col-md-3 text-right">
                <img src="assets/img/logo.gif" class="img-fluid" width="135" />
              </div>
            </div>
            <section id="receipt">
              <table class="table table-bordered">
                <thead>
                  <th colspan="2" class="text-center blue "style="font-size:25px">
                    Acknowledgement
                  </th>
                </thead>
                <tbody>
                  <tr>
                    <td style="width: 40%">Date</td>
                    <td><%=userData[0].submit_date%></td>
                  </tr>
                  <tr>
                    <td>Name of the Student</td>
                    <td><%=userData[0].cand_name%></td>
                  </tr>
                  <tr>
                    <td>Course</td>
                    <td><%=userData[0].course%></td>
                  </tr>
                  <tr>
                    <td>Student Id</td>
                    <td><%=userData[0].cand_id%></td>
                  </tr>
                  <tr>
                    <td>Amount Paid</td>
                    <td><%=userData[0].amount_paid%></td>
                  </tr>
                  <tr>
                    <td>Name of the Bank</td>
                    <td><%=userData[0].name_of_bank%></td>
                  </tr>
                  <tr>
                    <td>Transaction Id</td>
                    <td><%=userData[0].transaction_id%></td>
                  </tr>
                  <tr>
                    <td>Phone number</td>
                    <td><%=userData[0].mobile_phone%></td>
                  </tr>
                </tbody>
              </table>
            </section>
       
          </div>
          <section id="files">
            <div class="container mr-2"style="page-break-before:always">
              <h2 style="font-size: 25px;">Document to be submitted in the Admission Office on arrival at campus
              </h2>
              <h5 class="blue f-20"><u>Original Document</u>
              </h5>
              <ul>
                <li>Allotment Order
                </li>
                <li>NEET Score Card
                </li>
                <li>NEET Admit Card
                </li>
                <li>NEET Rank Letter
                </li>
                <li>X Standard Board Examination Mark Sheet
                </li>
                <li>X Standard Board Examination Pass Certificate (if applicable)
                </li>
                <li>XII Standard Board Examination Mark Sheet
                </li>
                <li>XII Standard Board Examination Pass Certificate (if applicable)
                </li>
                <li>School Transfer Certificate
                </li>
                <li>Community Certificate
                </li>
                <li>Conduct Certificate
                </li>
                <li>Migration Certificate (Except Tamilnadu State Board Candidates)
                </li>
                <li>Eligibility Certificate (Except Tamilnadu State Board Candidates)
                </li>
                <li>Hepatitis-b Vaccination Certificate	
                </li>
                <li>Passport sized Photo 4 Copies (Without Name Tag)
                </li>
      
              </ul>
              <h5 class="f-20"><u>Photocopy of the following documents</u>
              </h5>
              <ul>
                <li>Aadhar Card
                </li>
                <li>PAN Card
                </li>
                <li>Bank Pass Book First Page 
                </li>
              </ul>
            </div>
          </section>
        </div>
      </div>
    </section>
   
    <!-- jQuery bootstrap -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
      integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
      integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
      crossorigin="anonymous"
    ></script>
    <script
      type="text/javascript"
      src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.1.135/jspdf.min.js"
    ></script>
    <script>
      UPLOADCARE_PUBLIC_KEY = "demopublickey";
    </script>
    <script
      src="https://ucarecdn.com/libs/widget/3.x/uploadcare.full.min.js"
      charset="utf-8"
    ></script>
    <!-- AdminLTE App -->
    <script src="js/adminlte.js"></script>
    <script src="js/mbbs.js"></script>

    <!-- AdminLTE for demo purposes -->
    <script src="js/demo.js"></script>
    <!-- page script -->

    <script type="text/javascript">
      $(function () {
        var specialElementHandlers = {
          "#editor": function (element, renderer) {
            return true;
          },
        };
        $("#cmd").click(function () {
          var doc = new jsPDF();
          doc.fromHTML($("#report").html(), 15, 15, {
            width: 170,
            elementHandlers: specialElementHandlers,
          });
          doc.save("sample-file.pdf");
        });
      });
    </script>
    <script></script>
  </body>
</html>
