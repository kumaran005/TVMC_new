      document.getElementById('certificate_form').value = certificate_name[i].certificate_files_name;
    }
    if(certificate_name[i].certificate_type == 'Admission_Commitee_Form'){
      document.getElementById('admission_commitee_form').value = certificate_name[i].certificate_files_name;
    }
  }
} 

