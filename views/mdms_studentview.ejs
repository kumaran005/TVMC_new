            function updated(params) {
              if(params == 'saved'){
                document.getElementById('update_action').value = params;
                $('#updatesave').click();
              }else if(params == 'submitted'){
                  var transaction_id = document.getElementById('edit_transaction_id').value;
                var challan_file = document.getElementById('edit_challan_file').value;
                var challan_txt = document.getElementById('cand_challan').innerText;
                if(transaction_id){
                  if(challan_file || challan_txt){
                document.getElementById('update_action').value = params;
                $('#update_modal').modal('toggle');
                  }else{
                    $('#editTandC').modal('toggle');
                  }
                }else{
                   $('#editTandC').modal('toggle');
                }
                
              }
            }

            function Agreed(){
              $('#updatesave').click();
            }
